{
  "name": "puppeteer_crawler",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "seo": "node bin/deleteFolder.js report && mocha src/seo/runner.js --no-timeouts",
    "amazon": "node bin/deleteFolder.js report && node src/amazon/runner.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KenFront/puppeteer_crawler.git"
  },
  "author": "Ken",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KenFront/puppeteer_crawler/issues"
  },
  "homepage": "https://github.com/KenFront/puppeteer_crawler#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^5.2.0",
    "prettier": "^1.16.2",
    "puppeteer": "^1.11.0"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "prettier",
      "prettier/babel",
      "prettier/standard"
    ],
    "plugins": [
      "babel",
      "prettier",
      "standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": 10,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "eslintIgnore": [
    "report/**/",
    "node_modules/**/"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  }
}
